<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <system.webServer>
      <handlers>
	 <remove name="Python27_via_FastCGI" />
         <remove name="Python34_via_FastCGI" />
         <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Python36\python.exe|C:\Python36\Lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
      </handlers>
   </system.webServer>
   <appSettings file="C:\WEB\test\web.config">
      <!-- Required settings -->
      <add key="WSGI_HANDLER" value="booking.app" />
      <add key="PYTHONPATH" value="C:\WEB\Angular example" />
   </appSettings>
</configuration>